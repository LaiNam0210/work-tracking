{
  "version": 1,
  "projects": {
    "work-tracking": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/work-tracking",
      "sourceRoot": "apps/work-tracking/src",
      "prefix": "training",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/work-tracking",
            "index": "apps/work-tracking/src/index.html",
            "main": "apps/work-tracking/src/main.ts",
            "polyfills": "apps/work-tracking/src/polyfills.ts",
            "tsConfig": "apps/work-tracking/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/work-tracking/src/favicon.ico",
              "apps/work-tracking/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/work-tracking/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/work-tracking/src/environments/environment.ts",
                  "with": "apps/work-tracking/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "work-tracking:build",
            "proxyConfig": "apps/work-tracking/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "work-tracking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "work-tracking:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/work-tracking/tsconfig.app.json",
              "apps/work-tracking/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/work-tracking/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/work-tracking/jest.config.js",
            "tsConfig": "apps/work-tracking/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/work-tracking/src/test-setup.ts"
          }
        }
      }
    },
    "work-tracking-e2e": {
      "root": "apps/work-tracking-e2e",
      "sourceRoot": "apps/work-tracking-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/work-tracking-e2e/cypress.json",
            "tsConfig": "apps/work-tracking-e2e/tsconfig.e2e.json",
            "devServerTarget": "work-tracking:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "work-tracking:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/work-tracking-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/work-tracking-e2e/**"]
          }
        }
      }
    },
    "report": {
      "root": "libs/report",
      "sourceRoot": "libs/report/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/report/tsconfig.lib.json",
              "libs/report/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/report/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/report/jest.config.js",
            "tsConfig": "libs/report/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "work-tracking"
}
